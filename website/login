    <label for="AccountNumber">Username</label>
    <input type="email" placeholder="Please enter the email" id = "AccountNumber" >
    <br>
    <br>
    <label for="Password">Password</label>
    <input type="password" placeholder="Please enter the password" id = "Password">
    <br>
    <br>
    <button  onclick="btnClink1()"><a href="Campus%20Exploration.html">Log in</a></button>
    <p>Don't have an account?<a href="Register.html">Creat an account</a></p>
